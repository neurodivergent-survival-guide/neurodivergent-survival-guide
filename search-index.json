---
layout: null
---
[
{% assign struggle_pages = site.pages | where: "layout", "struggle" %}
{% for page in struggle_pages %}
  {% unless page.status == 'draft' %}
  {
    "title": {{ page.title | jsonify }},
    "subtitle": {{ page.subtitle | default: "" | jsonify }},
    "description": {{ page.description | default: "" | jsonify }},
    "url": {{ page.url | relative_url | jsonify }},
    "condition": {{ page.condition | default: "" | jsonify }},
    "search_terms": {{ page.search_terms | default: array | jsonify }},
    "slug": {{ page.slug | default: "" | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endunless %}
{% endfor %}
]
