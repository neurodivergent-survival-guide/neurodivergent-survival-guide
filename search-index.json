---
layout: null
---
[
  {% assign pages = site.pages | where_exp: "page", "page.layout == 'struggle'" %}
  {% for page in pages %}
    {% unless page.status == 'draft' %}
    {
      "title": "{{ page.title | escape }}",
      "subtitle": "{{ page.subtitle | escape }}",
      "description": "{{ page.description | escape }}",
      "url": "{{ page.url }}",
      "condition": "{{ page.condition }}",
      "search_terms": {{ page.search_terms | jsonify }},
      "slug": "{{ page.slug }}"
    }{% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]
